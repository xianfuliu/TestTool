  "interfaces": {
    "准入": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unQryAllowUser",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "custid": "ZY{request_id}",
        "userName": "{name}",
        "idNumber": "{id_card}",
        "mobileNo": "{phone}"
      }
    },
    "授信申请": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unCoopAppl",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "applyAmt": 0,
        "applyNo": "{request_id}",
        "applyTime": "{request_id}",
        "extraInfo": {
          "channelMark": "{channelMark}",
          "faceTime": "{request_id}",
          "faceType": "yuncong"
        },
        "userInfo": {
          "address": "云南省昆明市东川区汤丹镇新塘村委会新塘小组16号",
          "certNo": "{id_card}",
          "certType": "101",
          "certValidEnd": "20200406",
          "certValidStart": "20400406",
          "custId": "ZY{request_id}",
          "custName": "{name}",
          "emergList": [
            {
              "emergCertNo": "",
              "emergName": "好几斤",
              "emergRelation": "CHILDREN",
              "emergTel": "13791646646"
            }
          ],
          "gender": "1",
          "inctry": "156",
          "liveAddArea": "东城区",
          "liveAddCity": "北京市",
          "liveAddProvince": "北京市",
          "liveAddress": "减减肥劲夫劲夫那你都没",
          "monthIncome": "5",
          "phone": "{phone}",
          "workInfo": {
            "occupation": "1"
          }
        }
      }
    },
    "影像件上传": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/test/hl/uploadFile",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "applyNo": "{request_id}",
        "dateStr": "{date}"
      },
      "enable_encryption": false
    },
    "授信结果查询": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unQryCoopApplResult",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "applyNo": "{request_id}"
      },
      "response_mapping": {
        "zkj_amt": "crdtAmt",
        "zkj_lpr": "crdtRate"
      }
    },
    "终审结果通知": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unNotifyFinalApplResult",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "conditional_body": {
        "field": "credit_status",
        "cases": {
          "S": {
            "applyNo": "{request_id}",
            "status": "{credit_status}",
            "crdtNo": "CL{dateTime}",
            "crdtAmt": "25000.00",
            "effeDate": "{date}",
            "expiDate": "20950903",
            "intRate": "23.725000",
            "dayRate": 0.010822
          },
          "F": {
            "applyNo": "{request_id}",
            "status": "{credit_status}",
            "errCode": "D99",
            "errMsg": "资方授信审批拒绝"
          }
        }
      },
      "field_types": {
        "crdtAmt": "float",
        "intRate": "float",
        "dayRate": "float"
      }
    },
    "证件信息更新": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unUpdateCert",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "name": "{name}",
        "certNo": "{id_card}",
        "applyNo": "{request_id}",
        "applyTime": "{date}"
      }
    },
    "借款申请": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unCoopLoanAppl",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "applyNo": "{request_id}",
        "paymentApplyNo": "LN{request_id}{loan_num}",
        "custId": "ZY{request_id}",
        "applyTime": "{dateTime}",
        "applAmt": "{applAmt}",
        "applTnr": "{applTnr}",
        "repayType": "{repayType}",
        "loanPurpose": "{loanPurpose}",
        "payMode": "0",
        "repayCard": "{bank_card_number}",
        "acctName": "{name}",
        "channelMark": "{channelMark}"
      },
      "field_types": {
        "applAmt": "int"
      }
    },
    "借款结果查询": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unQryCoopLoanApplResult",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "body_template": {
        "paymentApplyNo": "LN{request_id}{loan_num}"
      },
      "response_mapping": {
        "guaranteeNo": "guaranteeNo"
      }
    },
    "放款结果通知": {
      "url": "https://xj-api-test.hqrzdb.com/stage-api/zy/api/unNotifyLendResult",
      "method": "POST",
      "headers": {
        "Content-Type": "application/json"
      },
      "conditional_body": {
        "field": "loan_status",
        "cases": {
          "S": {
            "status": "{loan_status}",
            "paymentApplyNo": "LN{request_id}{loan_num}",
            "channelMark": "{channelMark}",
            "loanNo": "ZYLOAN{dateTime}",
            "loanDate": "{date}",
            "loanAmt": "{applAmt}",
            "intRate": 3.95,
            "applTnr": "{applTnr}",
            "repytp": "{repayType}",
            "loanEndDate": "20260903",
            "loanTime": "{dateTime}",
            "paySignNum": "SIGN{dateTime}",
            "contractNO": "CONTRACT{dateTime}",
            "lprRate": 3.45,
            "lprValidDate": "{date}",
            "lprFloatPoint": 50.0
          },
          "F": {
            "status": "{loan_status}",
            "paymentApplyNo": "LN{request_id}{loan_num}",
            "channelMark": "{channelMark}",
            "errCode": "D99",
            "errMsg": "资方放款失败"
          }
        }
      },
      "field_types": {
        "loanAmt": "int"
      }
    }
  }
